<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.KFireSquad"
        tools:targetApi="31">
        <activity
            android:name=".activites.SellerProductRequestActivity"
            android:exported="false" />
        <activity
            android:name=".activites.SellerMarketPlaceActivity"
            android:exported="false" />
        <activity
            android:name=".activites.SellerUpdateProductActivity"
            android:exported="false" />
        <activity
            android:name=".activites.SellerProductViewActivity"
            android:exported="false" />
        <activity
            android:name=".activites.SellerProductsListActivity"
            android:exported="false" />
        <activity
            android:name=".activites.SellerAddProductActivity"
            android:exported="false" />
        <activity
            android:name=".activites.SellerProfileActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activites.SellerDetailsActivity"
            android:exported="false" />

    </application>

